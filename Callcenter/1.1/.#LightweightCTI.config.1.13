     <time hour="3" file="Voc_GoodMorning1"/>
     <time hour="4" file="Voc_GoodMorning1"/>
     <time hour="5" file="Voc_GoodMorning1"/>
     <time hour="6" file="Voc_GoodMorning1"/>
